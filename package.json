{
  "name": "rutils",
  "version": "0.5.0",
  "description": "A collection of Raspberry Pi utils",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-c": "jest --coverage",
    "start": "ts-node-dev src/index.ts",
    "prod": "ts-node src/index.ts NODE_ENV=production",
    "ssh-test": "ts-node src/modules/autoupdate/autoupdate.ts",
    "update:all": "git pull origin master && npm i && pm2 restart rutils && ts-node src/modules/autoupdate/autoupdate.ts",
    "update:self": "git pull origin master && npm i && pm2 restart rutils",
    "update:machamp:self": "cd /home/shelby/machamp && git pull origin master && npm i && pm2 restart machamp",
    "update:machamp:all": "npm run machamp:update:self && ts-node src/modules/autoupdate/machamp.ts",
    "pm2": "pm2 start npm --name=rutils -- run prod",
    "gen:schemas": "ts-node src/schema.generate.ts",
    "gen:services": "ts-node src/service.generate.ts",
    "gen:guards": "ts-node src/guard.generate.ts",
    "gen:all": "ts-node src/schema.generate.ts && ts-node src/service.generate.ts && ts-node src/guard.generate.ts",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MusicDev33/rutils.git"
  },
  "author": "Shelby McCowan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MusicDev33/rutils/issues"
  },
  "homepage": "https://github.com/MusicDev33/rutils#readme",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.15.0",
    "@types/amqplib": "^0.8.2",
    "@types/chart.js": "^2.9.34",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.4.1",
    "@types/node-cron": "^2.0.4",
    "amqplib": "^0.8.0",
    "apn": "^2.2.0",
    "axios": "^0.21.1",
    "chart.js": "^3.4.1",
    "child-process-async": "^1.0.1",
    "code-block-writer": "^10.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-defaults": "^2.0.2",
    "express": "^4.17.1",
    "jest": "^27.2.0",
    "js-stringify": "^1.0.2",
    "mongoose": "^5.13.3",
    "node-cron": "^3.0.0",
    "node-gyp": "^7.0.0",
    "node-ssh": "^12.0.0",
    "npm-check-updates": "^11.8.3",
    "npm-run-all": "^4.1.5",
    "ssh2-promise": "^1.0.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-morph": "^11.0.3",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "typescript": "^4.4.3"
  }
}
